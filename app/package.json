{
  "name": "app",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "commit": "git-cz",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:electron": "ng build --base-href ./",
    "start:electron": "concurrently \"ng serve\" \"wait-for-localhost 4200 && electron . --serve\"",
    "build:cordova": "(cd ../mobile && cordova emulate android)",
    "start:cordova": "nodemon --watch src --exec \"npm run build:electron && npm run build:cordova\" -e html,ts,css"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.3",
